      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.scss": [
      "prettier --write",
      "stylelint --fix"
    ],
    "./src/**/*.{js,jsx}": [
      "prettier --write",
      "vue-cli-service lint"
    ],
    "./src/**/*.vue": [
      "vue-cli-service lint"
    ]
  }
