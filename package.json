{
  "name": "boilerplate-vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "staging-build": "vue-cli-service build --report --mode staging",
    "build": "vue-cli-service build --report",
    "electron:dev": "vue-cli-service electron:serve",
    "electron:staging-build": "vue-cli-service electron:build --report --mode staging -m -w -l --x64",
    "electron:build": "vue-cli-service electron:build --report -m -w -l --x64",
    "check": "ncu",
    "commit": "git-cz && git push",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/i18n/locales/*.json'",
    "lint": "yarn lint:json && yarn lint:script && yarn lint:style && yarn lint:ls",
    "lint:json": "prettier ./**/*.json -w",
    "lint:script": "vue-cli-service lint --fix",
    "lint:style": "stylelint ./src/**/*.{css,less,scss,vue} --fix",
    "lint:ls": "ls-lint .",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@ant-design/colors": "~5.0.1",
    "@mdi/font": "~5.8.55",
    "@vue/composition-api": "~1.0.0-beta.22",
    "@vueuse/core": "~4.0.0",
    "axios": "~0.21.1",
    "better-scroll": "~2.0.6",
    "core-js": "~3.8.1",
    "dayjs": "~1.9.7",
    "lodash-es": "~4.17.20",
    "mitt": "~2.1.0",
    "mockjs": "~1.1.0",
    "nprogress": "~0.2.0",
    "portal-vue": "~2.1.7",
    "roboto-fontface": "~0.10.0",
    "swrv": "~0.9.1",
    "vue": "~2.6.12",
    "vue-i18n": "~8.22.2",
    "vue-router": "~3.4.9",
    "vuetify": "~2.4.0",
    "vuex": "~3.6.0",
    "xe-utils": "~3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "~11.0.0",
    "@intlify/vue-i18n-loader": "~1.0.0",
    "@ls-lint/ls-lint": "~1.9.2",
    "@modyqyw/fabric": "~1.8.0",
    "@types/lodash-es": "~4.17.4",
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-plugin-router": "~4.5.9",
    "@vue/cli-plugin-vuex": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "babel-eslint": "~10.1.0",
    "commitizen": "~4.2.2",
    "compression-webpack-plugin": "~6.1.1",
    "electron": "~8.5.5",
    "electron-devtools-installer": "~3.1.1",
    "eslint": "~7.16.0",
    "husky": "~4.3.6",
    "lint-staged": "~10.5.3",
    "npm-check-updates": "~10.2.5",
    "prettier": "~2.2.1",
    "sass": "~1.30.0",
    "sass-loader": "~10.1.0",
    "stylelint": "~13.8.0",
    "stylelint-webpack-plugin": "~2.1.1",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "~2.0.8",
    "vue-template-compiler": "~2.6.12",
    "vuetify-loader": "~1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12 <13 || >=14 <15"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    ".": "yarn lint"
  }
}
