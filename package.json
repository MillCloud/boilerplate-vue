{
  "name": "vue-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve",
    "build:staging": "vue-cli-service build --report --mode staging",
    "build": "vue-cli-service build --report",
    "lint": "npm run lint:script && npm run lint:style && npm run lint:ls",
    "check": "ncu",
    "commit": "git-cz && git push",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/i18n/locales/*.json'",
    "lint:script": "vue-cli-service lint",
    "lint:style": "stylelint ./src/**/*.scss --fix",
    "lint:ls": "ls-lint"
  },
  "dependencies": {
    "@ant-design/colors": "^4.0.5",
    "@ant-design/icons-svg": "^4.1.0",
    "@ant-design/icons-vue": "4.0.6",
    "@mdi/font": "^5.6.55",
    "@vue/composition-api": "^1.0.0-beta.14",
    "axios": "^0.20.0",
    "better-scroll": "^2.0.4",
    "core-js": "^3.6.5",
    "dayjs": "^1.8.36",
    "echarts": "^4.9.0",
    "element-ui": "^2.13.2",
    "element-ui-eoi": "^2.14.4",
    "lodash-es": "^4.17.15",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "portal-vue": "^2.1.7",
    "ress": "^3.0.0",
    "roboto-fontface": "*",
    "screenfull": "^5.0.2",
    "swrv": "^0.7.6",
    "vue": "^2.6.12",
    "vue-clipboard2": "^0.3.1",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-i18n": "^8.21.1",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.4.5",
    "vue-virtual-scroll-list": "^2.3.1",
    "vuetify": "^2.3.10",
    "vuex": "^3.5.1",
    "vxe-table": "^2.9.23",
    "vxe-table-plugin-element": "^1.9.3",
    "xe-utils": "^2.7.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@ls-lint/ls-lint": "^1.9.2",
    "@modyqyw/eslint-config-vue": "^1.7.6",
    "@modyqyw/prettier-config": "^1.1.3",
    "@modyqyw/stylelint-config-scss": "^1.1.0",
    "@types/lodash-es": "^4.17.3",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-router": "^4.5.6",
    "@vue/cli-plugin-vuex": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.1",
    "compression-webpack-plugin": "^6.0.2",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "npm-check-updates": "^9.0.2",
    "prettier": "^2.1.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "stylelint": "^13.7.2",
    "stylelint-webpack-plugin": "^2.1.0",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-tailwind": "^1.5.0",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.scss": [
      "prettier --write",
      "stylelint --fix"
    ],
    "./src/**/*.{js,jsx}": [
      "prettier --write",
      "vue-cli-service lint"
    ],
    "./src/**/*.vue": [
      "vue-cli-service lint",
      "stylelint --fix"
    ],
    ".": [
      "ls-lint"
    ]
  }
}
