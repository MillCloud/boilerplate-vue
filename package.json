    "vuex": "^3.5.1",
    "vxe-table": "^2.9.19",
    "vxe-table-plugin-element": "^1.9.1",
    "xe-utils": "^2.7.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
