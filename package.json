    "vue-template-compiler": "^2.6.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.scss": [
      "prettier --write",
      "stylelint --fix"
    ],
    "./src/**/*.{js,jsx}": [
      "prettier --write",
      "vue-cli-service lint"
    ],
    "./src/**/*.vue": [
      "vue-cli-service lint",
      "stylelint --fix"
    ]
  }
