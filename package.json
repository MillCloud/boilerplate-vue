    "axios": "^0.20.0",
    "better-scroll": "^2.0.3",
    "core-js": "^3.6.5",
    "dayjs": "^1.8.35",
    "element-ui": "^2.13.2",
    "element-ui-eoi": "^2.14.4",
    "lodash-es": "^4.17.15",
    "nprogress": "^0.2.0",
    "ress": "^3.0.0",
    "swrv": "^0.7.3",
    "vue": "^2.6.12",
    "vue-router": "^3.4.3",
    "vuex": "^3.5.1",
    "vxe-table": "^2.9.19",
    "vxe-table-plugin-element": "^1.9.1",
