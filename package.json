{
  "name": "boilerplate-vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3",
  "author": {
    "email": "",
    "name": "",
    "url": ""
  },
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "staging-build": "vue-cli-service build --mode staging --report",
    "build": "vue-cli-service build --mode production --report",
    "electron:dev": "vue-cli-service electron:serve",
    "electron:staging-build": "vue-cli-service electron:build --report --mode staging -m -w -l",
    "electron:build": "vue-cli-service electron:build --report -m -w -l",
    "check": "ncu",
    "commit": "cz",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/i18n/locales/**/*.json\"",
    "test:unit": "vue-cli-service test:unit --coverage",
    "lint": "yarn lint:json && yarn lint:markdown && yarn lint:script && yarn lint:style && yarn lint:ls",
    "lint:json": "prettier ./**/*.json --write",
    "lint:markdown": "markdownlint . --fix && lint-md . --fix",
    "lint:script": "vue-cli-service lint --fix",
    "lint:style": "stylelint ./**/*.{css,less,sass,scss,vue} --fix",
    "lint:ls": "ls-lint .",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prepare": "is-ci || husky install"
  },
  "main": "background.js",
  "dependencies": {
    "@vueuse/core": "~4.11.0",
    "@vueuse/integrations": "~4.11.0",
    "@vueuse/router": "~4.11.0",
    "axios": "~0.21.1",
    "core-js": "~3.12.1",
    "dayjs": "~1.10.4",
    "element-plus": "~1.0.2-beta.44",
    "lodash-es": "~4.17.21",
    "mitt": "~2.1.0",
    "mockjs": "~1.1.0",
    "nprogress": "~0.2.0",
    "ress": "~4.0.0",
    "vue": "~3.0.11",
    "vue-i18n": "~9.1.4",
    "vue-router": "~4.0.0",
    "vuex": "~4.0.0",
    "xe-utils": "~3.2.1"
  },
  "devDependencies": {
    "@babel/core": "~7.14.2",
    "@babel/eslint-parser": "~7.14.2",
    "@commitlint/cli": "~12.1.4",
    "@intlify/vue-i18n-loader": "~2.1.0",
    "@ls-lint/ls-lint": "~1.9.2",
    "@modyqyw/fabric": "~2.0.2",
    "@types/electron-devtools-installer": "~2.2.0",
    "@types/jest": "~26.0.23",
    "@types/lodash-es": "~4.17.4",
    "@types/node": "~15.3.0",
    "@types/nprogress": "~0.2.0",
    "@typescript-eslint/eslint-plugin": "~4.23.0",
    "@typescript-eslint/parser": "~4.23.0",
    "@vue/cli-plugin-babel": "~5.0.0-beta.1",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.1",
    "@vue/cli-plugin-router": "~5.0.0-beta.1",
    "@vue/cli-plugin-typescript": "~5.0.0-beta.1",
    "@vue/cli-plugin-unit-jest": "~5.0.0-beta.1",
    "@vue/cli-plugin-vuex": "~5.0.0-beta.1",
    "@vue/cli-service": "~5.0.0-beta.1",
    "@vue/compiler-sfc": "~3.0.11",
    "@vue/test-utils": "~2.0.0-rc.6",
    "commitizen": "~4.2.4",
    "compression-webpack-plugin": "~7.1.2",
    "electron": "~12.0.7",
    "electron-devtools-installer": "~3.2.0",
    "eslint": "~7.26.0",
    "husky": "~6.0.0",
    "is-ci": "~3.0.0",
    "lint-md-cli": "~0.1.2",
    "lint-staged": "~11.0.0",
    "markdownlint-cli": "~0.27.1",
    "npm-check-updates": "~11.5.12",
    "prettier": "~2.3.0",
    "sass": "~1.32.13",
    "sass-loader": "~11.1.1",
    "stylelint": "~13.13.1",
    "stylelint-webpack-plugin": "~2.1.1",
    "ts-jest": "~26.5.6",
    "typescript": "~4.2.4",
    "vue-cli-plugin-electron-builder": "~2.0.0",
    "vue-cli-plugin-element-plus": "~0.0.13",
    "vue-cli-plugin-i18n": "~2.1.0",
    "vue-jest": "~5.0.0-alpha.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12 <13"
  }
}
