{
  "name": "boilerplate-vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue2",
  "author": {
    "email": "",
    "name": "",
    "url": ""
  },
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "staging-build": "vue-cli-service build --mode staging --report",
    "build": "vue-cli-service build --mode production --report",
    "electron:dev": "vue-cli-service electron:serve",
    "electron:staging-build": "vue-cli-service electron:build --report --mode staging -m -w -l",
    "electron:build": "vue-cli-service electron:build --report -m -w -l",
    "check": "ncu",
    "commit": "cz",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/i18n/locales/*.json'",
    "test:unit": "vue-cli-service test:unit --coverage",
    "lint": "yarn lint:json && yarn lint:markdown && yarn lint:script && yarn lint:style && yarn lint:ls",
    "lint:json": "prettier ./**/*.json --write",
    "lint:markdown": "markdownlint . --fix && lint-md . --fix",
    "lint:script": "vue-cli-service lint --fix",
    "lint:style": "stylelint ./**/*.{css,less,sass,scss,vue} --fix",
    "lint:ls": "ls-lint .",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prepare": "is-ci || husky install"
  },
  "main": "background.js",
  "dependencies": {
    "@mdi/font": "~5.9.55",
    "axios": "~0.21.1",
    "better-scroll": "~2.2.1",
    "core-js": "~3.10.0",
    "dayjs": "~1.10.4",
    "lodash-es": "~4.17.21",
    "mockjs": "~1.1.0",
    "mitt": "~2.1.0",
    "nprogress": "~0.2.0",
    "portal-vue": "~2.1.7",
    "roboto-fontface": "~0.10.0",
    "vue": "~2.6.12",
    "vue-i18n": "~8.24.2",
    "vue-router": "~3.5.1",
    "vuetify": "~2.4.9",
    "vuex": "~3.6.2",
    "xe-utils": "~3.1.12"
  },
  "devDependencies": {
    "@babel/core": "~7.13.14",
    "@babel/eslint-parser": "~7.13.14",
    "@commitlint/cli": "~12.0.1",
    "@intlify/vue-i18n-loader": "~1.1.0",
    "@ls-lint/ls-lint": "~1.9.2",
    "@modyqyw/fabric": "~1.25.2",
    "@types/electron-devtools-installer": "~2.2.0",
    "@types/jest": "~26.0.22",
    "@types/lodash-es": "~4.17.4",
    "@types/node": "~14.14.37",
    "@types/nprogress": "~0.2.0",
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-eslint": "~4.5.12",
    "@vue/cli-plugin-router": "~4.5.12",
    "@vue/cli-plugin-unit-jest": "~4.5.12",
    "@vue/cli-plugin-vuex": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/test-utils": "~1.1.3",
    "commitizen": "~4.2.3",
    "compression-webpack-plugin": "~6.1.1",
    "electron": "~12.0.2",
    "electron-devtools-installer": "~3.1.1",
    "eslint": "~7.23.0",
    "husky": "~6.0.0",
    "is-ci": "~3.0.0",
    "lint-md-cli": "~0.1.2",
    "lint-staged": "~10.5.4",
    "markdownlint-cli": "~0.27.1",
    "npm-check-updates": "~11.3.0",
    "prettier": "~2.2.1",
    "sass": "~1.32.8",
    "sass-loader": "~10.1.1",
    "stylelint": "~13.12.0",
    "stylelint-webpack-plugin": "~2.1.1",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.6",
    "vue-cli-plugin-i18n": "~2.0.3",
    "vue-cli-plugin-vuetify": "~2.3.1",
    "vue-template-compiler": "~2.6.12",
    "vuetify-loader": "~1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12 <13"
  }
}
