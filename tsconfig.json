{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "@/*": ["./src/*"],
      "@a/*": ["./src/assets/*"],
      "@c/*": ["./src/components/*"],
      "@h/*": ["./src/hooks/*"],
      "@u/*": ["./src/utils/*"]
    },
    "lib": ["esnext", "scripthost"],
    "allowJs": true,
    "noEmit": true
  },
  "include": ["**/*"],
  "exclude": ["node_modules"]
}
